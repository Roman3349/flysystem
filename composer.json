{
  "name": "contributte/flysystem",
  "description": "Smart integration of Flysystem into Nette Framework.",
  "keywords": [
    "nette",
    "flysystem",
    "filesystem"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/flysystem",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "contributte/di": "^0.5.0",
    "league/flysystem": "^2.0 || ^3.0"
  },
  "require-dev": {
    "contributte/qa": "^0.3.2",
    "contributte/phpstan": "^0.2",
    "contributte/tester": "^0.3.0",
    "league/flysystem-memory": "^2.0 || ^3.0",
    "tracy/tracy": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Flysystem\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Flysystem\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
